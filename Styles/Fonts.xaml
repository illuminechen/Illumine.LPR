<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Illumine.LPR"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  <FontFamily x:Key="LatoRegular">
    pack://aplication;,,,/Fonts/#Lato Regular
  </FontFamily>
  <FontFamily x:Key="LatoThin">
    pack://aplication;,,,/Fonts/#Lato Thin
  </FontFamily>
  <FontFamily x:Key="LatoBold">
    pack://aplication;,,,/Fonts/#Lato Bold
  </FontFamily>
  <FontFamily x:Key="FontAwesome">
    pack://aplication;,,,/Fonts/#FontAwesome
  </FontFamily>
  <FontFamily x:Key="FontAwesome5">
    pack://aplication;,,,/Fonts/#Font Awesome 5 Free Solid
  </FontFamily>
  <Style x:Key="BaseStyle" TargetType="{x:Type Control}">
    <Setter Property="Control.FontFamily" Value="{StaticResource LatoThin}"/>
    <Setter Property="Control.FontSize" Value="{StaticResource FontSizeRegular}"/>
  </Style>
  <Style x:Key="BaseTextBlockStyle" TargetType="{x:Type TextBlock}">
    <Setter Property="TextBlock.FontFamily" Value="{StaticResource LatoThin}"/>
    <Setter Property="TextBlock.FontSize" Value="{StaticResource FontSizeRegular}"/>
  </Style>
  <Style TargetType="{x:Type Button}" BasedOn="{StaticResource BaseStyle}"/>
  <Style TargetType="{x:Type Label}" BasedOn="{StaticResource BaseStyle}"/>
  <Style TargetType="{x:Type TextBox}" BasedOn="{StaticResource BaseStyle}"/>
  <Style TargetType="{x:Type TextBlock}" BasedOn="{StaticResource BaseTextBlockStyle}"/>
  <Style TargetType="{x:Type ListView}" BasedOn="{StaticResource BaseStyle}"/>
  <system:Double x:Key="FontSizeSmaller">10</system:Double>
  <system:Double x:Key="FontSizeSmall">12</system:Double>
  <system:Double x:Key="FontSizeRegular">14</system:Double>
  <system:Double x:Key="FontSizeLarge">20</system:Double>
  <system:Double x:Key="FontSizeXLarge">24</system:Double>
  <system:Double x:Key="FontSizeXXLarge">30</system:Double>
  <system:Double x:Key="FontSizeXXXLarge">40</system:Double>
